{
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "regions": ["iad1"],
  "functions": {
    "src/app/api/stocks/[symbol]/route.ts": {
      "maxDuration": 10
    },
    "src/app/api/stocks/[symbol]/moat/route.ts": {
      "maxDuration": 30
    },
    "src/app/api/stocks/[symbol]/historical/route.ts": {
      "maxDuration": 10
    },
    "src/app/api/stocks/[symbol]/news/route.ts": {
      "maxDuration": 10
    }
  },
  "env": {
    "NODE_ENV": "production"
  },
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, s-maxage=60, stale-while-revalidate=300"
        }
      ]
    }
  ]
}
